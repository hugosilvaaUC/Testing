{
  "name": "@usercentrics/cmp-browser-sdk",
  "version": "2.8.1",
  "tcfCmpVersion": "3",
  "author": "Usercentrics GmbH",
  "description": "Usercentrics CMP Browser SDK",
  "homepage": "https://docs.usercentrics.com/cmp_browser_sdk/2.8.1/index.html",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/index.js",
  "mainUmd": "dist/umd/bundle.js",
  "mainUmdLegacy": "dist/umd/bundle_legacy.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/usercentricscode/cmp-browser-sdk/"
  },
  "files": [
    "dist",
    "dist/src"
  ],
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "commit": "git-cz",
    "docs": "rimraf docs && typedoc --out docs/$npm_package_version && sed s/\\$\\{VERSION\\}/$npm_package_version/ docs.usercentrics.com/cmp_browser_sdk/index.html > docs/index.html",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --ignore-pattern '.tmp/*'",
    "lint-fix": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --fix --ignore-pattern '.tmp/*'",
    "prebuild": "rimraf dist",
    "pre-commit": "lint-staged",
    "prepublishOnly": "npm run build",
    "start": "npm run start:cookie-server & npm run start:cross-domain-server & npm run start:rollup",
    "start:cookie-server": "./node_modules/.bin/http-server ./consensu.org --port 8090",
    "start:cross-domain-server": "./node_modules/.bin/http-server ./app.usercentrics.eu --port 8091",
    "start:rollup": "rollup -wc rollup.config.js",
    "test": "jest --no-cache",
    "test:coverage": "jest --coverage",
    "prepare-release": "./prepare-release.sh",
    "version": "echo $npm_package_version",
    "prepare": "husky install"
  },
  "dependencies": {
    "@iabtcf/cmpapi": "^1.3.0",
    "@iabtcf/core": "^1.3.0",
    "@iabtcf/stub": "^1.3.0",
    "@types/crypto-js": "^3.1.47",
    "@types/uuid": "^8.0.0",
    "crypto-js": "3.3.0",
    "dynamic-import-polyfill": "^0.1.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "commitizen": "^4.2.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "custom-event-polyfill": "^1.0.7",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.5",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.8",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-visualizer": "^4.2.0",
    "ts-jest": "^26.5.4",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.1.3",
    "whatwg-fetch": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write"
    ]
  }
}
